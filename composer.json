{
  "name": "codeinwp/themeisle-companion",
  "description": "Orbit Fox Companion",
  "keywords": [
    "wordpress"
  ],
  "homepage": "https://themeisle.com/plugins/themeisle-companion/",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "ThemeIsle Team",
      "email": "friends@themeisle.com.com",
      "homepage": "https://themeisle.com"
    }
  ],
  "type": "wordpress-plugin",
  "support": {
    "issues": "https://github.com/Codeinwp/themeisle-companion/issues",
    "source": "https://github.com/Codeinwp/themeisle-companion"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3",
    "wp-coding-standards/wpcs": "^1",
    "phpcompatibility/php-compatibility": "^9",
    "dealerdirect/phpcodesniffer-composer-installer": "0.4.4",
    "automattic/vipwpcs": "^0.3.0"
  },
  "scripts": {
    "lint": "phpcs",
    "format": "phpcbf"
  },
  "require": {
    "codeinwp/themeisle-sdk": "*", 
    "codeinwp/full-width-page-templates": "master",
    "codeinwp/elementor-extra-widgets": "master",
    "codeinwp/templates-directory": "master",
    "codeinwp/themeisle-content-forms": "master",
    "codeinwp/gutenberg-blocks": "dev-master"
  },
  "autoload": {
    "files": [
      "vendor/codeinwp/themeisle-sdk/load.php",
      "vendor/codeinwp/themeisle-content-forms/load.php"
    ]
  },
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "installer-disable": "true"
  },
  "minimum-stability": "dev"
}
